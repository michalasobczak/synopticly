Welcome, to the <strong>synopticly</strong>!

<br /><br />

<svg id="graphContainer">
  <g/>
</svg>

<br />

<textarea id="dot" style="width:600px;height:400px;display:none;">
digraph {
  <% Application.all.each do |app| %>
    <% if !app.app_name.blank? then %>
      <% desc = Entry.where("application_id = ? AND created_at >= now() - interval '1 minute'", app.id).count rescue nil %>
      <% exact = (desc / 60.0).round(2).to_s rescue '' %>
      <% rounded = (desc / 60.0).round(0).to_s rescue '' %>
      "<%= app.app_name %>" [label="<%= app.app_name + "\n" + "pods: #{rounded} (#{exact})" %>"];
    <% end %>
  <% end %>
}
</textarea>

<script>
  setInterval(function() {
    location.reload();
  }, 1000);
</script>
